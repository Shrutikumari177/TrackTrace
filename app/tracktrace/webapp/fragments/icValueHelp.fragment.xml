<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="productValueHelp"
        noDataText="Loading..."
        title="Select Material No"
        search="onValueHelpSearch"
        liveChange="onValueHelpSearch"
        confirm="onValueHelpSelectItem"
        cancel="onValueHelpClosevoy"
        items="{uniqueProductModel>/products}"
        initialFocus="SearchField">
        <StandardListItem 
            id="IDGenStandardListtem"
            title="{uniqueProductModel>batchId}"
            description=""
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>
