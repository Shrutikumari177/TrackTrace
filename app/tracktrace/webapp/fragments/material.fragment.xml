<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">  
    <SelectDialog
        id="_materialDialog"
        noDataText="Loading..."
        title="Select Material"
        liveChange=".onMaterialFilterSearch"
        search=".onMaterialFilterSearch"
        confirm="onMaterialValueHelpClose"
        cancel="onMaterialValueHelpClose"
        items="{materialModel>/value}"
        initialFocus="SearchField"
        multiSelect="false"
        contentWidth="480px"
        
    >
        
        <StandardListItem id="_IDGenStandardListItem" 
            
            title="{materialModel>MaterialDesc}"
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
            
    </SelectDialog>
</core:FragmentDefinition>