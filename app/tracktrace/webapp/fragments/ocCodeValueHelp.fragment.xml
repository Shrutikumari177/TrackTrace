<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="ocCodeValueHelp"
        noDataText="Loading..."
        title="Select OC ID"
        search="onOCValueHelpSearch"
        liveChange="onOCValueHelpSearch"
        confirm="onOCValueConfirmItem"
        cancel="onValueHelpClosevoy"
        items="{batchDataModel>/}"
        initialFocus="SearchField">
        <StandardListItem 
            title="{batchDataModel>OCID}"
            description=""
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>
