<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="BoxCodeBatchIdValuehelp"
        noDataText="Loading..."
        title="Select Material No"
        search="onBatchIdValueHelpSearch"
        liveChange="onBatchIdValueHelpSearch"
        confirm="onValueHelpBatchIdSelection"
        cancel="onValueHelpBatchIdSelection"
        items="{/zbatchno_track}"
        initialFocus="SearchField">
        <StandardListItem 
            id="IDGenStandardListItem"
            title="{BatchNo}"
            description=""
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>
