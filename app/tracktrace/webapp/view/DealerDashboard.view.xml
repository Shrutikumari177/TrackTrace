<mvc:View
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:ndc="sap.ndc"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    controllerName="tracktrace.controller.DealerDashboard"
    height="100%"
     xmlns:form="sap.ui.layout.form"
    xmlns:tnt="sap.tnt"  
>
<tnt:ToolHeader  id="DealerDash_toolheader" class="sapUiTinyMargin">
          
            <Title id="DealerDash_Title" textAlign="Center" text="Dealer Dashboard" wrapping="false">
                <layoutData>
                    <OverflowToolbarLayoutData id="DealerDash_OverflowToolbarLayoutData" priority="Disappear"/>
                </layoutData>
            </Title>
            <ToolbarSpacer id="DealerDash_ToolbarSpacer" />
            
        </tnt:ToolHeader>
    <l:ResponsiveSplitter
        id="_IDGenResponsiveSplitter1"
        defaultPane="default"
    >
        <l:PaneContainer
            id="_IDGenPaneContainer"
            resize=".onRootContainerResize"
        >
            <!--  First Pane: Fixed Height 20%  -->
            <l:SplitPane
                requiredParentWidth="400"
                id="default"
            >
                <l:layoutData>
                    <l:SplitterLayoutData
                        id="_IDGenSplitterLayoutData"
                        size="25%"
                    />
                </l:layoutData>
                <Panel
                    id="_IDGenPanel"
                    headerText="Outer Compartments"
                    height="100%"
                >
                    <Label
                        id="_IDGenLabel"
                        text="SCAN BARCODE"
                        class="sapUiSmallMarginEnd "
                        design="Bold"
                    />
                    <!-- Barcode Scanner Button -->
                    <ndc:BarcodeScannerButton
                        id="sampleBarcodeScannerButton"
                        width="10%"
                        scanSuccess="onScanSuccess"
                        scanFail="onScanError"
                        inputLiveUpdate="onScanLiveupdate"
                        class="sapUiSmallMarginBottom"
                    />
                    <!-- <VBox id="_IDGenVBox" >
                    <Label id="_IDGenLabel9" 
                    
                    text="Start Date"
                    
                    
                    />
                    <DatePicker id="_IDGenDatePicker1" 
                    
                    class="sapUiSmallMarginBottom"
                    />
                    <Label id="_IDGenLabel15" 
                    
                    text="End Date"
                    
                    
                    />
                    <DatePicker id="_IDGenDatePicker2" 
                    
                    
                    />
                    
                    </VBox>
                     -->

                    <!-- <Input
                        id="stp_Input"
                        width="100%"
                        showValueHelp="true"
                        valueHelpRequest="outerCompartment"
                        value=""
                        placeholder="Outer Compartment"
                    /> -->

                    <List                    
                        id="idlst"
                        growing="true"
                        growingScrollToLoad="true"
                        growingThreshold="5"
                        items = "{/OuterContainer}"
                    >
                        <ObjectListItem
                            id="_IDGenObjectListItem1"
                            title="Outer Compartment Number"
                            type="Navigation"
                            press="onSelectContract"
                        >
                            <ObjectAttribute
                                id="matnr"
                                text="Material: {OCID}"
                            />
                        </ObjectListItem>
                    </List>
                </Panel>
            </l:SplitPane>
            <!--  Middle Pane: Auto Height  -->
            <l:SplitPane
                requiredParentWidth="400"
                id="default1"
            >
                <l:layoutData>
                    <l:SplitterLayoutData
                        id="_IDGenSplitterLayoutData1"
                        size="auto"
                    />
                </l:layoutData>
                <Panel
                    id="_IDGenPanel1"
                    headerText="Outer Compartment Details"
                    height="100%"
                >
                <HBox class="dealerDetail_form">
                    <f:SimpleForm
                    id="_IDGenSimpleForm11"
                    editable="false"
                    layout="ResponsiveGridLayout"
                    singleContainerFullSize="false"
                    class="formClass"
                    width="100%"
                  >
    <f:content>
        
         <VBox>
        <HBox >
        <Label text="Outer Box ID :" />
        <Text text="{newModel>/OCID}" class="sapUiSmallMarginBegin" /></HBox>
        <HBox class="sapUiSmallMarginTop">
        <Label text="Batch No :" />
        <Text text="{newModel>/BatchID}" class="sapUiSmallMarginBegin" /></HBox>
         <HBox class="sapUiSmallMarginTop">
        <Label text="Material :" />
        <Text text="{newModel>/Material}" class="sapUiSmallMarginBegin" /></HBox> 
        </VBox>

    </f:content>
</f:SimpleForm>

                    <f:SimpleForm
                    id="_IDGenSimpleForm12"
                    editable="false"
                    layout="ResponsiveGridLayout"
                    singleContainerFullSize="false"
                    class="formClass"
                    width="100%"
                  >
    <f:content>
        <VBox>
        <HBox >
        <Label text="Box Unit :" />
        <Text text="{newModel>/Material}" class="sapUiSmallMarginBegin" /></HBox>
        <HBox class="sapUiSmallMarginTop">
        <Label text="Inner Compartment Unit :" />
        <Text text="{newModel>/Material}" class="sapUiSmallMarginBegin" /></HBox>
         <HBox class="sapUiSmallMarginTop">
        <Label text="Material Code :" />
        <Text text="{newModel>/Material}" class="sapUiSmallMarginBegin" /></HBox> 
        </VBox>
    </f:content>
</f:SimpleForm>
</HBox>


   

                      <Dialog
                            id="dealerDashboardDetailQrDialog"
                            title="QR Code"
                            stretchOnPhone="true"
                         >
                            <customHeader>
                                <Toolbar>
                                    <Title text="QR Code" />
                                    <ToolbarSpacer />
                                    <Button
                                        icon="sap-icon://decline"
                                        press="onCloseQRDialog"
                                    />
                                </Toolbar>
                            </customHeader>
                            <content>
                                <Image
                                    class="qrImage"
                                    id="dealerDashboardDetailsqrImage"
                                    src=""
                                    width="200px"
                                    height="200px"
                                />
                            </content>
                            <buttons>
                                <Button
                                    text="Print QR"
                                    press="onDealerDashboardPrintQR"
                                    type="Emphasized"
                                />
                            </buttons>
                        </Dialog> 


                         <Table
                        id="dealerDash_containerTable1"
                        width="auto"
                        contextualWidth="Auto"
                        class="borderedTable"
                        sticky="ColumnHeaders"
                        selectionChange="onSelectionChange"
                        items="{newArrayModel>/}"
                    >
                        <headerToolbar>
                            <OverflowToolbar id="enOverflowToolbar1">
                                <Title
                                    id="1enTitle2"
                                    text="Material Information"
                                    textAlign="Center"
                                    level="H2"
                                />
                                <ToolbarSpacer id="enToolbarSpacer2" />
                            </OverflowToolbar>
                        </headerToolbar>

                        <columns>
                          
                            <Column
                                id="enColu10"
                                width="70px"
                                hAlign="Center"
                                mergeDuplicates="true"
                            >
                                <Label
                                    id="enLabel0"
                                    text="IC ID"
                                />
                            </Column>
                            <Column
                                id="enColu1"
                                width="70px"
                                hAlign="Center"
                                mergeDuplicates="true"
                            >
                                <Label
                                    id="enLabedl2"
                                    text="Packaging"
                                />
                            </Column>
                            <Column
                                id="enColum"
                                width="70px"
                                hAlign="Center"
                            >
                                <Label
                                    id="enLabdel4"
                                    text="Product Code"
                                />
                            </Column>
                            <Column
                                id="enColun2"
                                width="70px"
                                hAlign="Center"
                            >
                                <Label
                                    id="enLabdel3"
                                    text="Expiry Date"
                                />
                            </Column>
                            <Column
                                id="enColun3"
                                width="70px"
                                hAlign="Center"
                            >
                                <Label
                                    id="enLabedl5"
                                    text="Manufacturing Date"
                                />
                            </Column>
                            

                        
                            <Column
                                id="enColum2"
                                width="70px"
                                hAlign="Center"
                                mergeDuplicates="true"
                            >
                                <Label
                                    id="enLabe5"
                                    text="Box QR Code"
                                    textAlign="Center"
                                />
                            </Column>
                            <Column
                                id="enColum3"
                                width="70px"
                                hAlign="Center"
                                mergeDuplicates="true"
                            >
                                <Label
                                    id="enLabe6"
                                    text="IC QR Code"
                                    textAlign="Center"
                                />
                            </Column>
                        </columns>

                        <ColumnListItem
                            id="_istIt1"
                            class="borderedRow"
                            press="onMaterialRowSelect"
                        >
                            <cells>
                                <Text
                                    id="_IDnText2w5"
                                    class="inputItem"
                                    text="{newArrayModel>ICID}"
                                />
                                <Text
                                    id="_IDnTextee20"
                                    class="inputItem"
                                    text="{newArrayModel>BoxSerialNo}"
                                />
                                <Text
                                    id="_IDnTextee19"
                                    class="inputItem"
                                    text="{newArrayModel>pOrder}"
                                />
                                <Text
                                    id="_IDnTexte20"
                                    class="inputItem"
                                    text="{newArrayModel>expDate}"
                                />
                                <Text
                                    id="_IDnTexet21"
                                    class="inputItem"
                                    text="{newArrayModel>mfgDate}"
                                />
                                <Link text="{newArrayModel>BoxQRCode}"  press="onDealerDashboardBoxQrCode" />
                                <Link text="{newArrayModel>ICQRCode}"  press="onDealerDashboardICQrCode" />
                            </cells>
                        </ColumnListItem>
                    </Table>
                </Panel>
            </l:SplitPane>
        </l:PaneContainer>
    </l:ResponsiveSplitter>
</mvc:View>
